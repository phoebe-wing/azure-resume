{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch .NET Functions",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}/backend/api/bin/Debug/net8.0/azure-resume.dll",
      "args": [],
      "cwd": "${workspaceFolder}/backend/api",
      "console": "integratedTerminal",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": "Attach to .NET Functions",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:azureFunctions.pickProcess}"
    }
  ]
}